<h1>Products</h1>

<table id="main" role="grid">
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Description</th>
    <th>Code</th>
    <th>Price</th>
    <th>Status</th>
    <th>Stock</th>
    <th>Category</th>
    <th>Thumbnails</th>
  </tr>
  {{#each payload}}
    <tr>
      <td>{{this._id}}</td>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.code}}</td>
      <td>{{this.price}}</td>
      <td>{{this.status}}</td>
      <td>{{this.stock}}</td>
      <td>{{this.category}}</td>
      <td>{{this.thumbnails}}</td>
    </tr>
  {{/each}}
</table>

